<h1>Users!</h1>

<%= link("New user", to: user_path(@conn, :new)) %>
<table class="table">
	<thead>
		<tr>
			<th>Name (Id)</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<%= for user <- @users do  %>
		<tr>
			<td><%= render("user.html", user: user) %></td>
			<td><%= link("View", to: user_path(@conn, :show, user.id)) %></td>
		</tr>
		<% end %>
	</tbody>
</table>

